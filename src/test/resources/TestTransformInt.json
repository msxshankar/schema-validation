{
    "artifact": {
        "name": "cdap-data-pipeline",
        "version": "6.8.3",
        "scope": "SYSTEM"
    },
    "description": "Data Pipeline Application",
    "name": "TestTransform",
    "change": {
        "description": ""
    },
    "parentVersion": "",
    "config": {
        "resources": {
            "memoryMB": 2048,
            "virtualCores": 1
        },
        "driverResources": {
            "memoryMB": 2048,
            "virtualCores": 1
        },
        "connections": [
            {
                "from": "TestSource",
                "to": "SchemaValidate"
            },
            {
                "from": "SchemaValidate",
                "to": "TestSink"
            },
            {
                "from": "SchemaValidate",
                "to": "ErrorCollector"
            },
            {
                "from": "ErrorCollector",
                "to": "TestError"
            }
        ],
        "comments": [],
        "postActions": [],
        "properties": {},
        "processTimingEnabled": true,
        "stageLoggingEnabled": false,
        "stages": [
            {
                "name": "TestSource",
                "plugin": {
                    "name": "File",
                    "type": "batchsource",
                    "label": "TestSource",
                    "artifact": {
                        "name": "core-plugins",
                        "version": "2.10.2",
                        "scope": "SYSTEM"
                    },
                    "properties": {
                        "referenceName": "TestSource",
                        "path": "/usr/data/cdap-env/source.csv",
                        "format": "csv",
                        "sampleSize": "1000",
                        "filenameOnly": "false",
                        "recursive": "false",
                        "ignoreNonExistingFolders": "false",
                        "fileEncoding": "UTF-8",
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"string\"}]}",
                        "enableQuotedValues": "false",
                        "skipHeader": "true"
                    }
                },
                "outputSchema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"string\"}]}",
                "id": "TestSource"
            },
            {
                "name": "SchemaValidate",
                "plugin": {
                    "name": "ExampleTransform",
                    "type": "transform",
                    "label": "SchemaValidate",
                    "artifact": {
                        "name": "example-transform",
                        "version": "1.1.0-SNAPSHOT",
                        "scope": "USER"
                    },
                    "properties": {
                        "myOption": "/usr/data/cdap-env/int-schema.json",
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"int\"}]}"
                    }
                },
                "outputSchema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"int\"}]}",
                "inputSchema": [
                    {
                        "name": "TestSource",
                        "schema": "{\"name\":\"etlSchemaBody\",\"type\":\"record\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"boolean\"}]}"
                    }
                ],
                "id": "SchemaValidate"
            },
            {
                "name": "TestSink",
                "plugin": {
                    "name": "File",
                    "type": "batchsink",
                    "label": "TestSink",
                    "artifact": {
                        "name": "core-plugins",
                        "version": "2.10.2",
                        "scope": "SYSTEM"
                    },
                    "properties": {
                        "referenceName": "TestSink",
                        "path": "/usr/data/cdap-env/sink/",
                        "suffix": "yyyy-MM-dd-HH-mm",
                        "format": "csv",
                        "schema": "{\"name\":\"etlSchemaBody\",\"type\":\"record\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"int\"}]}",
                        "writeHeader": "true"
                    }
                },
                "outputSchema": "{\"name\":\"etlSchemaBody\",\"type\":\"record\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"int\"}]}",
                "inputSchema": [
                    {
                        "name": "SchemaValidate",
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"int\"}]}"
                    }
                ],
                "id": "TestSink"
            },
            {
                "name": "ErrorCollector",
                "plugin": {
                    "name": "ErrorCollector",
                    "type": "errortransform",
                    "label": "ErrorCollector",
                    "artifact": {
                        "name": "core-plugins",
                        "version": "2.10.2",
                        "scope": "SYSTEM"
                    },
                    "properties": {
                        "messageField": "ValidationError",
                        "codeField": "code",
                        "stageField": "node"
                    }
                },
                "outputSchema": "{\"type\":\"record\",\"name\":\"erroretlSchemaBody\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"string\"},{\"name\":\"ValidationError\",\"type\":\"string\"},{\"name\":\"code\",\"type\":\"int\"},{\"name\":\"node\",\"type\":\"string\"}]}",
                "inputSchema": [
                    {
                        "name": "SchemaValidate",
                        "schema": "{\"name\":\"etlSchemaBody\",\"type\":\"record\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"boolean\"}]}"
                    }
                ],
                "id": "ErrorCollector"
            },
            {
                "name": "TestError",
                "plugin": {
                    "name": "File",
                    "type": "batchsink",
                    "label": "TestError",
                    "artifact": {
                        "name": "core-plugins",
                        "version": "2.10.2",
                        "scope": "SYSTEM"
                    },
                    "properties": {
                        "referenceName": "TestError",
                        "path": "/usr/data/cdap-env/error/",
                        "suffix": "yyyy-MM-dd-HH-mm",
                        "format": "csv",
                        "schema": "{\"name\":\"etlSchemaBody\",\"type\":\"record\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"string\"},{\"name\":\"ValidationError\",\"type\":\"string\"}]}",
                        "writeHeader": "true"
                    }
                },
                "outputSchema": "{\"name\":\"etlSchemaBody\",\"type\":\"record\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"string\"},{\"name\":\"ValidationError\",\"type\":\"string\"}]}",
                "inputSchema": [
                    {
                        "name": "ErrorCollector",
                        "schema": "{\"type\":\"record\",\"name\":\"erroretlSchemaBody\",\"fields\":[{\"name\":\"name\",\"type\":\"string\"},{\"name\":\"age\",\"type\":\"string\"},{\"name\":\"ValidationError\",\"type\":\"string\"},{\"name\":\"code\",\"type\":\"int\"},{\"name\":\"node\",\"type\":\"string\"}]}"
                    }
                ],
                "id": "TestError"
            }
        ],
        "schedule": "0 1 */1 * *",
        "engine": "spark",
        "numOfRecordsPreview": 100,
        "rangeRecordsPreview": {
            "min": 1,
            "max": "5000"
        },
        "description": "Data Pipeline Application",
        "maxConcurrentRuns": 1
    },
    "version": "-SNAPSHOT"
}